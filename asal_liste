sonuc=[]

for i in range(10,100):
    for b in range(2,i):
        if(i%b==0):
            break  #bi tane bölen bulduğunda dur anlamına geliyor yeniden arama
    else:       #for olmazsa anlamında 
        sonuc.append(i)
print(sonuc)






for i in range(1,100):
    flag=False
    for b in range(2,i):
        if(i%b==0):
            flag=True
            break  #bi tane bölen bulduğunda dur anlamına geliyor yeniden arama
    if (flag!=True):
        print(i)




