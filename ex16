import math

def asalMi(n):
    for i in range(2,int(math.sqrt(n))+1,1):
        if(n%i)==0:
            return False
    return True
a=int(input("sayı giriniz= "))
if(a<2):
    print("1den büyük sayı giriniz!")
else:
    if(asalMi(a)):
        print("sayı asal")
    else:
        print("asal değil")
